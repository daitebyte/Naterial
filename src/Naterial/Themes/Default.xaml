<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Naterial;component/Themes/Dimensions.xaml" />
        <ResourceDictionary Source="/Naterial;component/Themes/FocusVisual.xaml" />
        <ResourceDictionary Source="/Naterial;component/Themes/Fonts.xaml" />

        <ResourceDictionary Source="/Naterial;component/Themes/Hyperlink.xaml" />
        <ResourceDictionary Source="/Naterial;component/Themes/TextBlock.xaml" />
    </ResourceDictionary.MergedDictionaries>


    <Style x:Key="{x:Static SystemParameters.FocusVisualStyleKey}" BasedOn="{StaticResource Style.FocusVisualDefault}" />


    <Style TargetType="Hyperlink" BasedOn="{StaticResource Style.HyperlinkDefault}" />

</ResourceDictionary>
